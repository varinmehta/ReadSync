{
    "version": 2,
    "build": [
        {
            "src": "frontend/vite.config.js",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "frontend/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "frontend/$1"
        }
    ]
}